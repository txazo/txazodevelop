<?xml version="1.0" encoding="UTF-8"?>

<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">

	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="1000" eternal="false" overflowToDisk="true" diskPersistent="false"
		timeToIdleSeconds="600" timeToLiveSeconds="1800" memoryStoreEvictionPolicy="LRU" />

	<cache name="org.txazo.cache.EhCache" maxElementsInMemory="100" eternal="false" overflowToDisk="true"
		diskPersistent="false" timeToIdleSeconds="600" timeToLiveSeconds="1800" memoryStoreEvictionPolicy="LRU" />

	<cache name="org.txazo.cache.SpringEhCache" maxElementsInMemory="100" eternal="false" overflowToDisk="true"
		diskPersistent="false" timeToIdleSeconds="600" timeToLiveSeconds="1800" memoryStoreEvictionPolicy="LRU" />

</ehcache>